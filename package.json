  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "electron": "electron .",
    "electron-build": "ng build && electron .",
    "package-build": "electron-packager ./ TestPlayer --electron --platform=win32 --out ./electron-app --overwrite",
    "package:win": "electron-packager ./ TestPlayer --overwrite --platform=win32 --arch=ia32 --out ./electron-app --overwrite --icon=src/assets/img/logo_win.ico",
    "package:mac": "electron-packager ./ TestPlayer --overwrite --platform=darwin --arch=x64 --out ./electron-app  --overwrite --icon=src/assets/img/logo_mac.icns",
    "package:linux": "electron-packager ./ TestPlayer --overwrite --platform=linux --arch=x64 --out ./electron-app --overwrite --icon=src/assets/img/logo_win.ico",
    "e2e": "ng e2e"
  },
  "private": true,
